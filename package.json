{
	"name": "cadsvelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"update-types": "npx supabase gen types typescript --project-id \"ghyfiwjocihqmmtfocjd\" --schema public > src/lib/db/types.ts",
		"seed": "ts-node prisma/seed.ts"
	},
	"prisma": {
		"seed": "vite-node ./prisma/seed.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@types/bcrypt": "^5.0.0",
		"@types/node": "^20.4.8",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"date-picker-svelte": "^2.6.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.27",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.1.1",
		"supabase": "^1.82.6",
		"svelte": "^4.0.5",
		"svelte-backtotop": "^0.0.0",
		"svelte-check": "^3.4.3",
		"sveltekit-superforms": "^1.5.0",
		"tailwindcss": "^3.3.3",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.2",
		"vite-node": "^0.34.1",
		"zod": "^3.21.4",
		"zod-prisma": "^0.5.4"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.1.1",
		"bcrypt": "^5.1.0",
		"cookies": "^0.8.0",
		"daisyui": "^3.5.0"
	}
}
