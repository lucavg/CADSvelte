{
	"name": "cadsvelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"update-types": "npx supabase gen types typescript --project-id \"ghyfiwjocihqmmtfocjd\" --schema public > src/lib/db/types.ts",
		"seed": "ts-node prisma/seed.ts"
	},
	"prisma": {
		"seed": "vite-node ./prisma/seed.ts"
	},
	"devDependencies": {
		"@supabase/supabase-js": "^2.39.1",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-vercel": "^3.1.0",
		"@sveltejs/kit": "^1.30.3",
		"@types/bcrypt": "^5.0.2",
		"@types/node": "^20.10.5",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"date-picker-svelte": "^2.10.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.7.1",
		"supabase": "^1.123.4",
		"svelte": "^4.2.8",
		"svelte-backtotop": "^0.0.0",
		"svelte-check": "^3.6.2",
		"sveltekit-superforms": "^1.12.0",
		"tailwindcss": "^3.4.0",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^4.5.1",
		"vite-node": "^0.34.7",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.7.1",
		"bcrypt": "^5.1.1",
		"cookies": "^0.8.0",
		"daisyui": "^3.9.4",
		"svelte-select": "^5.8.1",
		"zod-prisma-types": "^3.1.6"
	}
}
